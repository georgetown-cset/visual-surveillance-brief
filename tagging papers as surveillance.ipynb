    "#filtering for tasks that overlap with our task areas, aren't subsets of them, and have >=5 papers\n",
    "#we do not find large or relevant-looking tasks with high overlap\n",
    "\n",
    "for key in u_term_dfs.keys():\n",
    "    print(key)\n",
    "    currdf = u_term_dfs[key]\n",
    "    not_subset = currdf['overlaps'] != currdf['task total']\n",
    "    not_small = currdf['task total'] >= 5\n",
    "    \n",
    "    currdf_filtered = currdf[not_small]\n",
    "    print(currdf_filtered.sort_values('likelihood_mid', ascending=True)[:20])\n",
    "    print(\"\")\n"
