   "metadata": {},
   "outputs": [],
   "source": [
    "#find tasks that often co-occur with a particular regular expression\n",
    "#this helps us find tasks that should be associated with a task area\n",
    "\n",
    "\n",
    "\n",
    "def count_tasks_instances_overlaps(term_list, paper_df, all_tasks = None, row_tasks = None, term_dict = None):\n",
    "    if(all_tasks is None):\n",
    "        all_tasks, task_counts, row_tasks = gen_row_tasks_all(paper_df)\n",
    "    \n",
    "    task_counts = {}\n",
    "    task_overlap_counts = {}\n",
    "    any_overlaps = {}\n",
    "\n",
    "    if term_dict is None:\n",
    "        term_dict = {}\n",
    "        for ref_term in term_list:\n",
    "            term_dict[ref_term] = [ref_term]\n",
    "            \n",
    "        \n",
    "    for ref_term in term_list:\n",
    "        if ref_term not in term_dict.keys():\n",
    "            term_dict[ref_term] = [ref_term]\n",
    "        \n",
    "    for term_fam in term_dict.keys():\n",
    "        any_overlaps[term_fam] = []\n",
    "    \n",
    "    \n",
    "    for row in all_tasks:\n",
    "        term_fam_matches = {}\n",
    "        for term_fam in term_dict.keys():\n",
    "            term_fam_matches[term_fam] = False\n",
    "            for task in row:\n",
    "                for ref_term in term_dict[term_fam]:\n",
    "                    if(re.search(ref_term, task, flags=re.IGNORECASE)): \n",
    "                        term_fam_matches[term_fam] = True\n",
    "                        break\n",
    "                if(term_fam_matches[term_fam] == True): break\n",
    "                    \n",
    "        for task in row:\n",
    "            curr_overlaps = {}\n",
    "            for term_fam in term_dict.keys():\n",
    "                curr_overlaps[term_fam] = 0\n",
    "                if(term_fam_matches[term_fam]):\n",
    "                    curr_overlaps[term_fam] = 1\n",
    "                    if task not in any_overlaps[term_fam]:\n",
    "                        any_overlaps[term_fam].append(task)\n",
    "            if task not in task_counts.keys():\n",
    "                task_counts[task] = 1\n",
    "                task_overlap_counts[task] = dict(curr_overlaps)\n",
    "            else:\n",
    "                task_counts[task] += 1\n",
    "                for term_fam in term_dict.keys():\n",
    "                    task_overlap_counts[task][term_fam] += curr_overlaps[term_fam]\n",
    "    \n",
    "    \n",
    "    \n",
    "    return task_counts, task_overlap_counts, any_overlaps\n",
    "    \n",
    "    \n",
    "#given the outputs of the above function,\n",
    "#for each task area, generate a dataframe of overlap-tasks that overlap with that task area\n",
    "#including their # papers, the number of papers that overlap, the likelihood of overlap (overlap size / number of papers)\n",
    "#and how close the likelihood is to .5\n",
    "    #because if you sort by likelihood and don't filter out tasks that match the regex or that only have one paper,\n",
    "    #you get a lot of tasks with likelihood ~1 that you already know about or that don't matter.\n",
    "    #and filtering for likelihood ~.5 seems like a good way to catch task areas that are similar-but-not-overlapping\n",
    "\n",
    "def find_most_overlapping_tasks(term_list, task_counts, task_overlap_counts, any_overlaps, all_pad=1, overlaps_pad=0, exclude_regexmatches = False, term_dict = None):\n",
    "    \n",
    "    all_dfs = {}\n",
    "    \n",
    "    if term_dict is None:\n",
    "        term_dict = {}\n",
    "        for ref_term in term_list:\n",
    "            term_dict[ref_term] = [ref_term]\n",
    "        \n",
    "    for ref_term in term_list:\n",
    "        if ref_term not in term_dict.keys():\n",
    "            term_dict[ref_term] = [ref_term]\n",
    "    \n",
    "    for term_fam in term_dict.keys():\n",
    "        if term_fam not in any_overlaps.keys():\n",
    "            continue\n",
    "        \n",
    "        task_dict = {}\n",
    "        \n",
    "        for task in any_overlaps[term_fam]:\n",
    "            if(exclude_regexmatches):\n",
    "                #skip task if it regexmatches the search term\n",
    "                is_regexmatch = False\n",
    "                for term in term_dict[term_fam]:\n",
    "                    if(re.search(term, task, flags=re.IGNORECASE)):\n",
    "                        is_regexmatch = True\n",
    "                        break\n",
    "                        \n",
    "                \n",
    "                if(is_regexmatch):\n",
    "                    continue\n",
    "                    \n",
    "                    \n",
    "            task_total = task_counts[task]\n",
    "            task_overlaps = task_overlap_counts[task][term_fam]\n",
    "            likelihood = (task_overlaps+overlaps_pad)*1./(task_total+all_pad)\n",
    "            likelihood_mid = (.5-likelihood)**2 \n",
    "            task_dict[task] = [likelihood, task_overlaps, task_total, likelihood_mid]\n",
    "        task_df = pd.DataFrame.from_dict(task_dict, orient='index', columns = ['likelihood', 'overlaps', 'task total', 'likelihood_mid'])\n",
    "        all_dfs[term_fam] = task_df.sort_values('overlaps', ascending=False)\n",
    "    \n",
    "    return all_dfs\n",
    "    "
   ]
  },
