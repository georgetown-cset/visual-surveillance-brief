   "source": [
    "#generate data table with paper_id, year, country\n",
    "#and count for how many of a paper's tasks match each of the task areas\n",
    "#we also calculate \"total matches\", which is the sum of the matches across all tasks\n",
    "\n",
    "#note - our later analysis only cares whether a task appears in a paper or not, not about the count\n",
    "#so the fact that we count task occurrences here isn't relevant.\n",
    "\n",
    "\n",
    "unified_term_parsing = unified_term_dictionary\n",
    "all_terms = list(unified_term_parsing.keys()) #task area titles\n",
    "\n",
    "paper_term_dict = {}\n",
    "paper_term_columns = ['article_id', 'Year', 'Country'] + all_terms + ['total matches']\n",
    "\n",
    "for ind, row in paper_df.iterrows():\n",
    "    curr_id = row['cset_id']\n",
    "    curr_year = row['year']\n",
    "    curr_country = row['country']\n",
    "    curr_row = [curr_year, curr_country]\n",
    "    curr_tasks = eval(row['tasks'])\n",
    "    \n",
    "    total_matches = 0\n",
    "    \n",
    "    for term_fam in all_terms:\n",
    "        match = 0\n",
    "        term_list = unified_term_parsing[term_fam]\n",
    "        for task in curr_tasks:\n",
    "            for term in term_list:\n",
    "                if(re.search(term, task, flags=re.IGNORECASE)):\n",
    "                    match += 1 #eh, we'll keep track of the count\n",
    "        total_matches += match\n",
    "        curr_row.append(match)\n",
    "        \n",
    "    curr_row.append(total_matches)\n",
    "        \n",
    "    paper_term_dict[curr_id] = curr_row\n",
    "    \n",
    "    "
   ]
